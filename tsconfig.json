{
  "include": [
    "src/index.ts"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "lib"
  ],
  "compilerOptions": {
    "target": "es2015",
    "module": "es2020",
    "moduleResolution": "Node",
    "downlevelIteration": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "jsx": "react",
    "jsxFactory": "Inactive.createElement",
    "jsxFragmentFactory": "Inactive.Fragment",
    "outDir": "./dist/",
    "rootDir": "./src/",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
