{
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "lib"
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "downlevelIteration": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "jsx": "react",
    "jsxFactory": "Reactless.createElement",
    "jsxFragmentFactory": "Reactless.Fragment",
    "outDir": "./dist/",
    "rootDir": "./src/",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
